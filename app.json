{
  "expo": {
    "name": "HyMatch",
    "slug": "hymatch",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router", 
      "expo-font", 
      "expo-web-browser",
      [
        "expo-image-picker",
        {
          "photosPermission": "ギャラリーにアクセスしてプロフィール写真を選択する権限が必要です。"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
